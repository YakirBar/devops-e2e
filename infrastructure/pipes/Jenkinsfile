pipeline {
    agent {
        label 'inbound-agent'
    }

    stages {
        stage('Docker Images') {
            steps {
                sh 'docker images'
            }
        }
        stage('Docker Pull') {
            steps {
                sh 'docker pull nginx:latest'
            }
        }
        stage('Docker Images') {
            steps {
                sh 'docker images'
            }
        }
    }
}
